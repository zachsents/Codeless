{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "outputs": [
                ".next/**"
            ],
            "dependsOn": [
                "^build"
            ]
        },
        "dev": {
            "cache": false
        },
        "deploy": {
            "dependsOn": [
                "build"
            ]
        }
    }
}