{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "test": {
            "dependsOn": [
                "^test"
            ]
        },
        "build": {
            "dependsOn": [
                "test",
                "^build"
            ]
        },
        "dev": {
            "cache": false
        },
        "deploy": {
            "dependsOn": [
                "build"
            ]
        }
    }
}